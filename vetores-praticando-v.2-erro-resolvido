#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main (void){

	setlocale(LC_ALL,"");
	
	int w = 4, h = 4;

	int m[w][h] , i, j;

	for (i = 0; i < w; i++) {
		for (j = 0; j < h; j++) {
			printf("Digite o valor da linha %d, coluna %d da matriz:\n",i,j);
			scanf("%d", &m[i][j]);
		}
	}

	printf("\nOs valores na matriz sÃ£o:\n");
	for (i = 0; i < w; i++) {
		for (j = 0; j < h; j++) {
			printf("%d\t", m[i][j]);
		}
		printf("\n");
	}

	return 0;
}
